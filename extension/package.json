  "publisher": "Traycer",
  "activationEvents": [
    "onStartupFinished",
    "onView:traycerCommentNavigator",
    "onView:traycerDetailView",
    "onUri"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "walkthroughs": [
      {
        "id": "traycer.walkthrough",
        "title": "Traycer Walkthrough",
        "description": "Learn how to use Traycer to manage complex multi-file changes with precision and ease.",
        "steps": [
          {
            "id": "1_select_mode",
            "title": "Select Your Workflow",
            "description": "Choose how you want to build:\n\n- **Phases**: Start with a short chat to clarify your goal. Traycer then breaks your project into clear, actionable phases, each with its own plan.\n- **Plan**: Generate a single, detailed plan. Refine it with AI, then execute it using your preferred coding agent.\n- **Review**: Analyze recent changes to catch issues, track deviations, and maintain quality.",
            "media": {
              "image": "resources/assets/walkthrough/1-select-workflow.png",
              "altText": "Select Your Workflow"
            }
          },
          {
            "id": "2_describe_task",
            "title": "Describe Your Task",
            "description": "Tell Traycer what you want to build.\n\nReference files or folders using `@[NAME]`, attach images or snippets for context, and hit **Send** when you're ready.",
            "media": {
              "image": "resources/assets/walkthrough/2-describe-task.png",
              "altText": "Describe Your Task"
            }
          },
          {
            "id": "3_keep_building",
            "title": "Keep Building",
            "description": "Once your workflow begins, stay in flow:\n\n- Follow the generated phases or plan.\n- Iterate and validate with Traycer's feedback.\n- Keep building; every step compounds progress.",
            "media": {
              "image": "resources/assets/walkthrough/3-follow-workflow.png",
              "altText": "Keep Building"
            }
          },
          {
            "id": "4_connect_mcp",
            "title": "Connect MCP Servers",
            "description": "Click the **MCP icon** (top-right) to view and manage your Model Context Protocol servers.\n\nThese servers link Traycer with external context, extending what your AI can see and reason about.",
            "media": {
              "image": "resources/assets/walkthrough/4-mcp.png",
              "altText": "Connect MCP Servers"
            }
          },
          {
            "id": "5_customize_prompts",
            "title": "Customize Prompts & Agents",
            "description": "Open the **‚ãØ menu** (top-right) to fine-tune how Traycer thinks:\n\n- Edit prompt templates for planning or review.\n- Connect more AI agents, Claude Code, Gemini, Codex, Copilot, RooCode, Cursor, Windsurf, and beyond.\n- Adjust other settings to fit your workflow.",
            "media": {
              "image": "resources/assets/walkthrough/5-prompt-and-settings.png",
              "altText": "Customize Prompts & Agents"
            }
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "typescript",
        "extensions": [
          ".ts"
        ],
        "aliases": [
          "TypeScript",
          "ts"
        ]
      },
      {
        "id": "typescriptreact",
        "extensions": [
          ".tsx"
        ],
        "aliases": [
          "TypeScript React",
          "tsx"
        ]
      },
      {
        "id": "javascript",
        "extensions": [
          ".js",
          ".mjs",
          ".cjs",
          ".jsx"
        ],
        "aliases": [
          "JavaScript",
          "JavaScript React",
          "js",
          "mjs",
          "cjs",
          "jsx"
        ]
      },
      {
        "id": "python",
        "extensions": [
          ".py"
        ],
        "aliases": [
          "Python",
          "py"
        ]
      },
      {
        "id": "go",
        "extensions": [
          ".go"
        ],
        "aliases": [
          "Go",
          "go"
        ]
      },
      {
        "id": "rust",
        "extensions": [
          ".rs"
        ],
        "aliases": [
          "Rust",
          "rs"
        ]
      },
      {
        "id": "php",
        "extensions": [
          ".php"
        ],
        "aliases": [
          "PHP",
          "php"
        ]
      }
    ],
    "configuration": {
      "title": "Traycer",
      "properties": {
        "traycer.enablePromptTemplateSelector": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable or disable the prompt template selector when choosing an execution agent from the dropdown.",
          "scope": "window",
          "order": 10
        },
        "traycer.autoOpenDiffOnApply": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Automatically open the diff view when changes are applied.",
          "scope": "window",
          "order": 20
        },
        "traycer.alwaysAllowPayToRun": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Proceed with pay to run without prompting for confirmation.",
          "scope": "window",
          "order": 30
        },
        "traycer.additionalAgents": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string",
            "enum": [
              "Claude Code Extension",
              "Codex Extension",
              "Cline",
              "Kilo Code",
              "Roo Code",
              "Augment",
              "ZenCoder",
              "Amp",
              "Export as Markdown"
            ],
            "enumDescriptions": [
              "VS Code extension for Claude Code",
              "VS Code extension for OpenAI Codex",
              "Cline AI coding assistant",
              "Kilo Code AI assistant",
              "Roo Code AI assistant",
              "Augment Code AI coding assistant",
              "ZenCoder AI assistant",
              "Amp by Sourcegraph AI assistant",
              "Export conversation as Markdown file"
            ]
          },
          "markdownDescription": "Additional agents that will appear in the Execute in dropdown. Default agents (IDE, Traycer, Copy) are always available.",
          "scope": "window",
          "order": 31
        },
        "traycer.outputLevel": {
          "type": "string",
          "default": "info",
          "enum": [
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "enumDescriptions": [
            "Logs only errors.",
            "Logs errors and warnings.",
            "Logs errors, warnings, and messages.",
            "Logs verbose errors, warnings, and messages. Best for issue reporting.",
            "Logs all messages, including verbose messages. Best for debugging."
          ],
          "markdownDescription": "Specifies how much (if any) output will be sent to the Traycer output channel.",
          "scope": "window",
          "order": 40
        },
        "traycer.sendKey": {
          "type": "string",
          "default": "ctrlEnter",
          "enum": [
            "enter",
            "ctrlEnter"
          ],
          "enumItemLabels": [
            "Enter",
            "Ctrl/Cmd + Enter"
          ],
          "enumDescriptions": [
            "Pressing `Enter` key sends query.",
            "Pressing `Ctrl/Cmd + Enter` key sends query."
          ],
          "markdownDescription": "Specifies the submit key(s) for queries.",
          "scope": "window",
          "order": 50
        },
        "traycer.languagePreference": {
          "type": "string",
          "default": "English",
          "enum": [
            "Catal√†",
            "Deutsch",
            "English",
            "Espa√±ol",
            "Fran√ßais",
            "‡§π‡§ø‡§®‡•ç‡§¶‡•Ä",
            "Bahasa Indonesia",
            "Italiano",
            "Êó•Êú¨Ë™û",
            "ÌïúÍµ≠Ïñ¥",
            "Nederlands",
            "Polski",
            "Portugu√™s",
            "–†—É—Å—Å–∫–∏–π",
            "T√ºrk√ße",
            "Ti·∫øng Vi·ªát",
            "ÁÆÄ‰Ωì‰∏≠Êñá",
            "ÁπÅÈ´î‰∏≠Êñá"
          ],
          "markdownDescription": "**_Beta Feature_**: Specifies the language preference for AI responses and interactions.",
