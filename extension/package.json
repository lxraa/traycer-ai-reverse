    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "traycerCommentNavigator",
          "title": "Traycer",
          "icon": "resources/assets/traycer-icon.svg"
        }
      ]
    },
    "views": {
      "traycerCommentNavigator": [
        {
          "type": "webview",
          "id": "traycer.commentNavigatorWebview",
          "name": "Traycer",
          "accessibilityHelpContent": "Navigate through the comments generated by Traycer",
          "icon": "resources/assets/traycer-icon.svg"
        }
      ]
    },
    "submenus": [
      {
        "id": "traycer.main",
        "label": "Traycer"
      }
    ],
    "commands": [
      {
        "command": "traycer.triggerManualAnalysisFile",
        "title": "Analyze File",
        "category": "Traycer",
        "icon": "$(traycer-logo)"
      },
      {
        "command": "traycer.triggerManualAnalysisChanges",
        "title": "Analyze Changes in File",
        "category": "Traycer"
      },
      {
        "command": "traycer.triggerManualAnalysisAllChanges",
        "title": "Analyze Changes in Workspace",
        "category": "Traycer"
      },
      {
        "command": "traycer.signIn",
        "title": "Sign in",
        "category": "Traycer",
        "icon": "$(sign-in)"
      },
      {
        "command": "traycer.signOut",
        "title": "Sign Out",
        "category": "Traycer",
        "icon": "$(sign-out)"
      },
      {
        "command": "traycer.startNewTask",
        "title": "Start New Task",
        "category": "Traycer",
        "icon": "$(plus)"
      },
      {
        "command": "traycer.openSettings",
        "title": "Open Settings",
        "category": "Traycer",
        "icon": "$(settings-gear)"
      },
      {
        "command": "traycer.openTaskHistory",
        "title": "Task History",
        "category": "Traycer",
        "icon": "$(history)"
      },
      {
        "command": "traycer.listMCPServers",
        "title": "Remote MCP Servers",
        "category": "Traycer",
        "icon": {
          "dark": "resources/assets/mcp-dark.svg",
          "light": "resources/assets/mcp-light.svg"
        }
      },
      {
        "command": "traycer.managePromptTemplates",
        "title": "Manage Prompt Templates",
        "category": "Traycer",
        "icon": "$(settings-gear)"
      },
      {
        "command": "traycer.manageCLIAgents",
        "title": "Manage CLI Agents",
        "category": "Traycer",
        "icon": "$(tools)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "traycer.startNewTask",
          "group": "navigation@1",
          "when": "view == traycer.commentNavigatorWebview && traycer.enableCommands"
        },
        {
          "command": "traycer.openTaskHistory",
          "group": "navigation@2",
          "when": "view == traycer.commentNavigatorWebview && traycer.enableCommands"
        },
        {
          "command": "traycer.listMCPServers",
          "group": "navigation@3",
          "when": "view == traycer.commentNavigatorWebview && traycer.enableCommands"
        },
        {
          "command": "traycer.openSettings",
          "group": "overflow@1",
          "when": "view == traycer.commentNavigatorWebview && traycer.enableCommands"
        },
        {
          "command": "traycer.managePromptTemplates",
          "group": "overflow@2",
          "when": "view == traycer.commentNavigatorWebview && traycer.enableCommands"
        },
        {
          "command": "traycer.manageCLIAgents",
          "group": "overflow@3",
          "when": "view == traycer.commentNavigatorWebview && traycer.enableCommands"
        }
      ],
      "editor/context": [
        {
          "when": "traycer.enableCommands",
          "submenu": "traycer.main",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "traycer.triggerManualAnalysisFile",
          "group": "navigation",
          "when": "traycer.enableCommands"
        }
      ],
      "traycer.main": [
        {
          "command": "traycer.triggerManualAnalysisFile",
          "group": "traycerMenuGroup@3",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.triggerManualAnalysisChanges",
          "group": "traycerMenuGroup@1",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.triggerManualAnalysisAllChanges",
          "group": "traycerMenuGroup@2",
          "when": "traycer.enableCommands"
        }
      ],
      "commandPalette": [
        {
          "command": "traycer.triggerManualAnalysisFile",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.triggerManualAnalysisChanges",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.triggerManualAnalysisAllChanges",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.signIn",
          "when": "traycer.isSignedOut"
        },
        {
          "command": "traycer.signOut",
          "when": "!traycer.isSignedOut"
        },
        {
          "command": "traycer.startNewTask",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.openTaskHistory",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.listMCPServers",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.openSettings",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.managePromptTemplates",
          "when": "traycer.enableCommands"
        },
        {
          "command": "traycer.manageCLIAgents",
          "when": "traycer.enableCommands"
        }
      ]
    },
    "icons": {
      "traycer-logo": {
        "description": "Traycer Logo",
        "default": {
          "fontPath": "resources/assets/Avatar_Black.woff",
          "fontCharacter": "\\e900"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify && node build-script.mjs",
    "preesbuild-base": "rm -rf out/ && cd traycer-views && npm run build && cd ..",
    "esbuild-base": "esbuild ../resources/tiktokenWorkerPool.mjs --bundle --outfile=./resources/tiktokenWorkerPool.js --external:vscode --format=cjs --platform=node && node scripts/esbuild.mjs",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "build": "npm run esbuild",
    "compile": "tsc -p ./",
    "watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch:all": "concurrently \"npm run esbuild-base -- --sourcemap --watch\" \"npm run watch --workspace traycer-views\"",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint . --ext .ts,.tsx --cache --fix",
    "test": "vscode-test",
    "format": "find . -type f \\( -name '*.ts' -o -name '*.js' -o -name '*.json' -o -name '*.jsx' -o -name '*.tsx' \\) -not -path './node_modules/*' -not -path './out/*' -not -path './dist/*' -not -path './.next/*' -not -path './@types/*' -not -path './traycer-ui/*' -not -path './traycer-ui-vite/*' | xargs npx prettier --write",
    "prestaging_deploy": "rm -rf *.vsix",
    "staging_deploy": "bash ./scripts/staging_deploy.sh",
    "production_deploy": "bash ./scripts/production_deploy.sh",
    "local_uninstall": "code --uninstall-extension traycer.traycer-vscode"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0"
  }
}
