{
  "name": "traycer-vscode",
  "displayName": "Traycer",
  "description": "An AI-powered coding assistant that plans, implements, and reviews every change üöÄ",
  "author": {
    "name": "Traycer",
    "email": "support@traycer.ai"
  },
  "pricing": "Free",
  "preview": false,
  "icon": "resources/assets/traycer-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/traycerai/community"
  },
  "version": "2.13.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "bugs": {
    "url": "https://discord.gg/QYDzEyT7Fq",
    "email": "support@traycer.ai"
  },
  "qna": "https://discord.gg/FA62AjTqCa",
  "categories": [
    "AI",
    "Chat",
    "Machine Learning",
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "openai",
    "codex",
    "pilot",
    "snippets",
    "intellisense",
    "refactor",
    "python",
    "typescript",
    "typescriptreact",
    "tsx",
    "javascript",
    "javascriptreact",
    "jsx",
    "golang",
    "rust",
